{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "target": "es6",
    "moduleResolution": "node",
    "lib": ["es6", "dom"],
    "types": ["cypress"],
    "noImplicitAny": false,
    "baseUrl": "../../",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["../../node_modules/cypress", "**/*.ts"]
}
