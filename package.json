  },
  "private": false,
  "license": "GNU GPLv3",
  "main": "dist/alloy-map.js",
  "types": "types/index.d.ts",
  "files": [
    "types/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "test:start-server": "http-server ./ -p 8888 -c-1 --silent",
    "test:cypress-ci": "cypress run --record --key d23922fd-ccda-44fe-8857-2b0206885367",
    "test:cypress-verify": "cypress verify",
    "test:cypress": "cypress open",
    "test:ci": "npm run build & start-server-and-test test:start-server http://localhost:8888 test:cypress-ci",
    "test": "npm run build & start-server-and-test test:start-server http://localhost:8888 test:cypress",
    "test:unit": "ts-mocha -r esm -p tests/unit/tsconfig.json --config .mocharc.json --paths",
    "pack": "webpack --config webpack.config.js",
    "build": "npm run eslint & npm run stylelint & npm run pack",
    "postbuild": "npx tscpaths -p tsconfig.types.json -s ./types -o ./types",
    "docs": "typedoc ./src",
    "codegen": "node ./submodules/alloy-typescript-codegen/copyToLocal.js ./submodules/alloy-typescript-codegen/dist ./src/api",
    "eslint": "eslint src/{**/,}*.{ts,js}",
    "eslint:fix": "eslint --fix src/{**/,}*.{ts,js}",
    "stylelint": "stylelint src/assets/less/{**/,}*.{css,less}",
    "stylelint:fix": "stylelint --fix src/assets/less/{**/,}*.{css,less}"
  },
  "dependencies": {
    "@yottaltd/eslint-config": "^1.0.1",
    "@yottaltd/eslint-plugin": "^1.1.1",
    "color": "^3.1.3",
    "debug": "^4.3.1",
    "dompurify": "^2.2.6",
    "geojson": "^0.5.0",
    "lodash": "^4.17.21",
    "ol": "^6.3.1",
    "portable-fetch": "^3.0.0",
    "proj4": "^2.7.0",
    "ste-simple-events": "^2.0.9",
    "uuid": "^8.3.2",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.6.0",
    "@types/chai": "^4.2.15",
    "@types/color": "^3.0.1",
    "@types/debug": "^4.1.5",
    "@types/dompurify": "^2.2.1",
    "@types/geojson": "^7946.0.7",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@types/ol": "^6.3.1",
    "@types/uuid": "^8.3.0",
    "@types/webfontloader": "^1.6.32",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "chai": "^4.3.0",
    "css-loader": "^5.1.0",
    "cypress": "^6.5.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.3.1",
    "esm": "^3.2.25",
    "http-server": "^0.12.3",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "mocha": "^8.3.0",
    "npm": "^7.5.6",
    "prettier": "^2.1.2",
    "start-server-and-test": "^1.12.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.11.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "ts-loader": "^8.0.17",
    "ts-mocha": "^8.0.0",
    "tscpaths": "0.0.9",
    "typedoc": "^0.20.28",
    "typescript": "^3.7.3",
    "webpack": "^4.41.4",
