{
  "name": "@yotta/alloy-map",
  "version": "1.0.48",
  "repository": {
    "type": "git",
    "url": "https://github.com/yottaltd/alloy-map.git"
  },
  "author": {
    "email": "craig.mcnicholas@weareyotta.com",
    "name": "Craig McNicholas",
    "url": "https://weareyotta.com/"
  },
  "private": false,
  "license": "GNU GPLv3",
  "main": "dist/alloy-map.js",
  "types": "types/index.d.ts",
  "files": [
    "types/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "test:start-server": "http-server ./ -p 8888 -c-1 --silent",
    "test:cypress-ci": "cypress run --record --key d23922fd-ccda-44fe-8857-2b0206885367",
    "test:cypress-verify": "cypress verify",
    "test:cypress": "cypress open",
    "test:ci": "npm run build & start-server-and-test test:start-server http://localhost:8888 test:cypress-ci",
    "test": "npm run build & start-server-and-test test:start-server http://localhost:8888 test:cypress",
    "test:unit": "ts-mocha -r esm -p tests/unit/tsconfig.json --config .mocharc.json --paths",
    "pack": "webpack --config webpack.config.js",
    "build": "npm run eslint & npm run stylelint & npm run pack",
    "postbuild": "npx tscpaths -p tsconfig.types.json -s ./types -o ./types",
    "docs": "typedoc ./src",
    "codegen": "node ./submodules/alloy-typescript-codegen/copyToLocal.js ./submodules/alloy-typescript-codegen/dist ./src/api",
    "eslint": "eslint src/{**/,}*.{ts,js}",
    "eslint:fix": "eslint --fix src/{**/,}*.{ts,js}",
    "stylelint": "stylelint src/assets/less/{**/,}*.{css,less}",
    "stylelint:fix": "stylelint --fix src/assets/less/{**/,}*.{css,less}"
  },
  "dependencies": {
    "color": "^3.1.2",
    "debug": "^4.1.1",
    "dompurify": "^2.0.7",
    "geojson": "^0.5.0",
    "lodash": "^4.17.15",
    "ol": "^6.3.1",
    "portable-fetch": "^3.0.0",
    "proj4": "^2.6.0",
    "ste-simple-events": "^1.6.3",
    "uuid": "^3.3.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.4.2",
    "@types/chai": "^4.2.7",
    "@types/color": "^3.0.0",
    "@types/debug": "^4.1.5",
    "@types/dompurify": "0.0.33",
    "@types/geojson": "^7946.0.7",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.9.1",
    "@types/ol": "^6.3.1",
    "@types/uuid": "^3.4.6",
    "@types/webfontloader": "^1.6.29",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "chai": "^4.2.0",
    "css-loader": "^3.5.2",
    "cypress": "^4.12.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-prettier": "^3.1.2",
    "esm": "^3.2.25",
    "http-server": "^0.12.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mocha": "^6.2.3",
    "npm": "^6.13.4",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.10.6",
    "style-loader": "^1.1.4",
    "stylelint": "^13.3.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "ts-loader": "^5.4.5",
    "ts-mocha": "^7.0.0",
    "tscpaths": "0.0.9",
    "typedoc": "^0.17.0",
    "typescript": "^3.7.3",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ]
